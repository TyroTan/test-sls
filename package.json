  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/jest": "^25.2.1",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^13.13.25",
    "@types/sharp": "^0.25.0",
    "awesome-phonenumber": "^2.31.0",
    "aws-sdk": "^2.770.0",
    "axios": "^0.19.2",
    "bwip-js": "^2.0.6",
    "canvas": "^2.6.1",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "jest": "^26.0.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "prettier": "^2.1.2",
    "serverless-prune-plugin": "^1.4.2",
    "serverless-pseudo-parameters": "^2.5.0",
    "serverless-step-functions": "^2.18.0",
    "serverless-webpack": "^5.3.2",
    "sharp": "^0.25.2",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "rootDir": "src",
    "globals": {
      "ts-jest": {
        "tsConfig": "./test-config.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "coverageThreshold": {
